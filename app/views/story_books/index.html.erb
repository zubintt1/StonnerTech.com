<p id="notice"><%= notice %></p>

<h1>Story Books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @story_books.each do |story_book| %>
      <tr>
        <td><%= link_to 'Show', story_book %></td>
        <td><%= link_to 'Edit', edit_story_book_path(story_book) %></td>
        <td><%= link_to 'Destroy', story_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Story Book', new_story_book_path %>
